:import "spec"

:actor Main
  :new (env)
    Specs.run(env, [
      SpecRun(ArraySpec).new(env)
      SpecRun(BoolSpec).new(env)
      SpecRun(CPointerSpec).new(env)
      SpecRun(InspectSpec).new(env)
      SpecRun(NumericSpec).new(env)
      SpecRun(PlatformSpec).new(env)
      SpecRun(StringSpec).new(env)
    ])
