:class Unicode.Spec
  :is Spec
  :const describes: "Unicode"

  :it "converts a codepoint to lowercase or uppercase if possible"
    // Note: the following two websites are useful when working on these tests:
    // Search by pasted character: https://www.compart.com/en/unicode
    // View codepoints as a table: https://unicode-table.com/en/

    // 0x00...0x80 : ASCII / Basic Latin
    assert error: Unicode.codepoint_to_lowercase!('@')
    assert error: Unicode.codepoint_to_uppercase!('@')
    assert:       Unicode.codepoint_to_lowercase!('A') == 'a'
    assert error: Unicode.codepoint_to_uppercase!('A')
    assert:       Unicode.codepoint_to_lowercase!('Z') == 'z'
    assert error: Unicode.codepoint_to_uppercase!('Z')
    assert error: Unicode.codepoint_to_lowercase!('[')
    assert error: Unicode.codepoint_to_uppercase!('[')
    assert error: Unicode.codepoint_to_lowercase!('`')
    assert error: Unicode.codepoint_to_uppercase!('`')
    assert error: Unicode.codepoint_to_lowercase!('a')
    assert:       Unicode.codepoint_to_uppercase!('a') == 'A'
    assert error: Unicode.codepoint_to_lowercase!('z')
    assert:       Unicode.codepoint_to_uppercase!('z') == 'Z'
    assert error: Unicode.codepoint_to_lowercase!('{')
    assert error: Unicode.codepoint_to_uppercase!('{')

    // 0x00xx : Latin-1 Supplement
    assert error: Unicode.codepoint_to_lowercase!('¬ø')
    assert error: Unicode.codepoint_to_uppercase!('¬ø')
    assert:       Unicode.codepoint_to_lowercase!('√Ä') == '√†'
    assert error: Unicode.codepoint_to_uppercase!('√Ä')
    assert:       Unicode.codepoint_to_lowercase!('√ñ') == '√∂'
    assert error: Unicode.codepoint_to_uppercase!('√ñ')
    assert error: Unicode.codepoint_to_lowercase!('√ó')
    assert error: Unicode.codepoint_to_uppercase!('√ó')
    assert:       Unicode.codepoint_to_lowercase!('√ò') == '√∏'
    assert error: Unicode.codepoint_to_uppercase!('√ò')
    assert:       Unicode.codepoint_to_lowercase!('√û') == '√æ'
    assert error: Unicode.codepoint_to_uppercase!('√û')
    assert error: Unicode.codepoint_to_lowercase!('√ü')
    assert error: Unicode.codepoint_to_uppercase!('√ü')
    assert error: Unicode.codepoint_to_lowercase!('√†')
    assert:       Unicode.codepoint_to_uppercase!('√†') == '√Ä'
    assert error: Unicode.codepoint_to_lowercase!('√∂')
    assert:       Unicode.codepoint_to_uppercase!('√∂') == '√ñ'
    assert error: Unicode.codepoint_to_lowercase!('√∑')
    assert error: Unicode.codepoint_to_uppercase!('√∑')
    assert error: Unicode.codepoint_to_lowercase!('√∏')
    assert:       Unicode.codepoint_to_uppercase!('√∏') == '√ò'
    assert error: Unicode.codepoint_to_lowercase!('√æ')
    assert:       Unicode.codepoint_to_uppercase!('√æ') == '√û'
    assert error: Unicode.codepoint_to_lowercase!('√ø')
    assert:       Unicode.codepoint_to_uppercase!('√ø') == '≈∏'

    // 0x01xx : Latin Extended-A and (part of) Extended-B
    // 0x02xx : (part of) Latin Extended-B
    assert:       Unicode.codepoint_to_lowercase!('ƒÄ') == 'ƒÅ'
    assert:       Unicode.codepoint_to_uppercase!('ƒÅ') == 'ƒÄ'
    assert:       Unicode.codepoint_to_lowercase!('ƒÆ') == 'ƒØ'
    assert:       Unicode.codepoint_to_uppercase!('ƒØ') == 'ƒÆ'
    assert error: Unicode.codepoint_to_lowercase!('ƒ∞')
    assert error: Unicode.codepoint_to_uppercase!('ƒ∞')
    assert:       Unicode.codepoint_to_uppercase!('ƒ±') == 'I' // uses ASCII 'I' uppercase
    assert:       Unicode.codepoint_to_lowercase!('ƒ≤') == 'ƒ≥'
    assert:       Unicode.codepoint_to_uppercase!('ƒ≥') == 'ƒ≤'
    assert:       Unicode.codepoint_to_lowercase!('ƒ∂') == 'ƒ∑'
    assert:       Unicode.codepoint_to_uppercase!('ƒ∑') == 'ƒ∂'
    assert error: Unicode.codepoint_to_lowercase!('ƒ∏')
    assert error: Unicode.codepoint_to_uppercase!('ƒ∏')
    assert:       Unicode.codepoint_to_lowercase!('ƒπ') == 'ƒ∫'
    assert:       Unicode.codepoint_to_uppercase!('ƒ∫') == 'ƒπ'
    assert:       Unicode.codepoint_to_lowercase!('≈á') == '≈à'
    assert:       Unicode.codepoint_to_uppercase!('≈à') == '≈á'
    assert error: Unicode.codepoint_to_lowercase!('≈â')
    assert error: Unicode.codepoint_to_uppercase!('≈â')
    assert:       Unicode.codepoint_to_lowercase!('≈ä') == '≈ã'
    assert:       Unicode.codepoint_to_uppercase!('≈ã') == '≈ä'
    assert:       Unicode.codepoint_to_lowercase!('≈∂') == '≈∑'
    assert:       Unicode.codepoint_to_uppercase!('≈∑') == '≈∂'
    assert:       Unicode.codepoint_to_lowercase!('≈∏') == '√ø'
    assert:       Unicode.codepoint_to_uppercase!('√ø') == '≈∏'
    assert:       Unicode.codepoint_to_lowercase!('≈π') == '≈∫'
    assert:       Unicode.codepoint_to_uppercase!('≈∫') == '≈π'
    assert:       Unicode.codepoint_to_lowercase!('≈Ω') == '≈æ'
    assert:       Unicode.codepoint_to_uppercase!('≈æ') == '≈Ω'
    assert error: Unicode.codepoint_to_lowercase!('≈ø')
    assert error: Unicode.codepoint_to_uppercase!('≈ø')
    assert:       Unicode.codepoint_to_lowercase!('∆Å') == '…ì'
    assert:       Unicode.codepoint_to_uppercase!('…ì') == '∆Å'
    assert:       Unicode.codepoint_to_lowercase!('∆Ç') == '∆É'
    assert:       Unicode.codepoint_to_uppercase!('∆É') == '∆Ç'
    assert:       Unicode.codepoint_to_lowercase!('∆Ü') == '…î'
    assert:       Unicode.codepoint_to_uppercase!('…î') == '∆Ü'
    assert:       Unicode.codepoint_to_lowercase!('∆á') == '∆à'
    assert:       Unicode.codepoint_to_uppercase!('∆à') == '∆á'
    assert error: Unicode.codepoint_to_lowercase!('∆â')
    assert error: Unicode.codepoint_to_uppercase!('∆â')
    assert:       Unicode.codepoint_to_lowercase!('∆ä') == '…ó'
    assert:       Unicode.codepoint_to_uppercase!('…ó') == '∆ä'
    assert:       Unicode.codepoint_to_lowercase!('∆ã') == '∆å'
    assert:       Unicode.codepoint_to_uppercase!('∆å') == '∆ã'
    assert error: Unicode.codepoint_to_lowercase!('∆ç')
    assert error: Unicode.codepoint_to_uppercase!('∆ç')
    assert:       Unicode.codepoint_to_lowercase!('∆é') == '…ò'
    assert:       Unicode.codepoint_to_uppercase!('…ò') == '∆é'
    assert:       Unicode.codepoint_to_lowercase!('∆è') == '…ô'
    assert:       Unicode.codepoint_to_uppercase!('…ô') == '∆è'
    assert:       Unicode.codepoint_to_lowercase!('∆ê') == '…õ'
    assert:       Unicode.codepoint_to_uppercase!('…õ') == '∆ê'
    assert:       Unicode.codepoint_to_lowercase!('∆ë') == '∆í'
    assert:       Unicode.codepoint_to_uppercase!('∆í') == '∆ë'
    assert:       Unicode.codepoint_to_lowercase!('∆ì') == '…†'
    assert:       Unicode.codepoint_to_uppercase!('…†') == '∆ì'
    assert:       Unicode.codepoint_to_lowercase!('∆î') == '…£'
    assert:       Unicode.codepoint_to_uppercase!('…£') == '∆î'
    assert:       Unicode.codepoint_to_lowercase!('∆ñ') == '…©'
    assert:       Unicode.codepoint_to_uppercase!('…©') == '∆ñ'
    assert:       Unicode.codepoint_to_lowercase!('∆ó') == '…®'
    assert:       Unicode.codepoint_to_uppercase!('…®') == '∆ó'
    assert:       Unicode.codepoint_to_lowercase!('∆ò') == '∆ô'
    assert:       Unicode.codepoint_to_uppercase!('∆ô') == '∆ò'
    assert error: Unicode.codepoint_to_uppercase!('∆õ')
    assert error: Unicode.codepoint_to_lowercase!('∆õ')
    assert:       Unicode.codepoint_to_lowercase!('∆ú') == '…Ø'
    assert:       Unicode.codepoint_to_uppercase!('…Ø') == '∆ú'
    assert:       Unicode.codepoint_to_lowercase!('∆ù') == '…≤'
    assert:       Unicode.codepoint_to_uppercase!('…≤') == '∆ù'
    assert:       Unicode.codepoint_to_lowercase!('»†') == '∆û'
    assert:       Unicode.codepoint_to_uppercase!('∆û') == '»†'
    assert:       Unicode.codepoint_to_lowercase!('∆ü') == '…µ'
    assert:       Unicode.codepoint_to_uppercase!('…µ') == '∆ü'
    assert:       Unicode.codepoint_to_lowercase!('∆†') == '∆°'
