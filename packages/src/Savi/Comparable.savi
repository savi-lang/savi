:trait Comparable(A Comparable(A)'read)
  :fun "=="(other A'box) Bool // TODO: use :is Equatable(A) instead
  :fun "<"(other A'box) Bool
  :fun "<="(other A'box) Bool: (@ == other) || (@ < other)
  :fun ">="(other A'box) Bool: (@ < other).is_false
  :fun ">"(other A'box) Bool: (@ <= other).is_false
